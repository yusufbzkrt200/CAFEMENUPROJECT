@using CAFEMENUPROJECT.DATA.Entity
@model Property
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<div class="card card-primary">
    <form>
        <div class="card-body">
            <input type="hidden" id="IdUpdate" value="@Model.Id"/>
            <div class="form-group">
                <label for="Key">Key</label>
                <input type="text" class="form-control" id="KeyUpdate" placeholder="Enter Key" value="@Model.Key">
            </div>

            <div class="form-group">
                <label for="Value">Value</label>
                <input type="text" class="form-control" id="ValueUpdate" placeholder="Enter Value" value="@Model.Value">
            </div>

        </div>
        <!-- /.card-body -->

    </form>
</div>


